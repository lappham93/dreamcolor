                                        <li>
                                            <a href="{{URI_PP}}" data-rel="colorbox">
                                                <img width="150" height="150" alt="150x150" src="{{URI_PP}}" />
                                                <div class="text">
                                                    <div class="inner">Primary Photo</div>
                                                </div>
                                            </a>

                                            <div class="tools tools-bottom">
                                                <a href="#" onclick="changeImgModal('{{ID}}'); return false;">
                                                    <i class="ace-icon fa fa-camera"></i>
                                                    <span style="font-size: 12px;">Change image</span>
                                                </a>
                                            </div>
                                        </li>
                                    </ul>
                                </td>
                                <td class="hidden-md hidden-lg">{{NAME}}</td>
                                <!--<td class="hidden-480">{{PRODUCT}}</td>-->
