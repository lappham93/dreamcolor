    <div class="tab-content">
        <div id="table" class="tab-pane fade in active">

            <h3 class="header smaller lighter blue">Add SKU</h3>
            <div class="row">
                <div class="col-xs-12">
                    <iframe id="if_Add" name="if_Add" style="display: none;width: 0px;height: 0px"></iframe>
                    <form id="frmAdd" name="frmAdd" class="form-horizontal" role="form" action="/web/admin/sku" method="POST" enctype="multipart/form-data" target="if_Add">
                        <div id="add_alert_success" class="alert alert-success" style="display: none;"></div>
                        <div id="add_alert_err" class="alert alert-danger" style="display: none;"></div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Name: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="name" name="name" placeholder="name" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1-1">Product: <span class="orange">(*)</span></label>
                            <div class="col-xs-12 col-sm-9">
                                <div class="col-xs-12 col-sm-5" style="margin-left: 0px; margin-right: 0px; padding-left: 0px; padding-right: 0px;">
                                    <select class="col-xs-12 col-sm-12" id="pid" name="pid" data-placeholder="Choose a product...">
                                        <option value=""> Choose a product... </option>
                                        {{#loop_option_product}}
                                        <option value="{{KEY}}">{{VALUE}}</option>
                                        {{/loop_option_product}}
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Base Price: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="bprice" name="bprice" placeholder="base price" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">UpSale Percent Distributor: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="upd" name="upd" placeholder="upsale percent distributor" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">UpSale Percent Salon Buyer: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="upsb" name="upsb" placeholder="upsale percent salon Buyer" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">UpSale Percent Retailer: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="upr" name="upr" placeholder="upsale percent retailer" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Quantity: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="qty" name="qty" placeholder="quantity" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Description: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="desc" name="desc" placeholder="description" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 col-xs-5 control-label no-padding-right" for="form-field-1">Display Order: <span class="orange">(*)</span></label>
<!--                            <div class="col-sm-9">
                                <input type="text" id="order" name="order" placeholder="display order" class="col-xs-12 col-sm-5"/>
                            </div>-->
                            <div class="col-sm-9 col-xs-7">
                                <input type="text" id="order" name="order" />
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1-1">Status: <span class="orange">(*)</span></label>
                            <div class="col-xs-12 col-sm-9">
                                <div class="col-xs-12 col-sm-5" style="margin-left: 0px; margin-right: 0px; padding-left: 0px; padding-right: 0px;">
                                    <select class="col-xs-12 col-sm-12" id="status" name="status" data-placeholder="Choose a status...">
                                        <option value=""> Choose a status... </option>
                                        <option value="0">Delete</option>
                                        <option value="1">Active</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Width: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="width" name="width" placeholder="width" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Height: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="height" name="height" placeholder="height" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div><div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Depth: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="depth" name="depth" placeholder="depth" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1-1">Dimension Unit: <span class="orange">(*)</span></label>
                            <div class="col-xs-12 col-sm-9">
                                <div class="col-xs-12 col-sm-5" style="margin-left: 0px; margin-right: 0px; padding-left: 0px; padding-right: 0px;">
                                    <select class="col-xs-12 col-sm-12" id="dunit" name="dunit" data-placeholder="Choose a unit...">
                                        <option value=""> Choose a unit... </option>
                                        <option value="0">Inch</option>
                                        <option value="1">Feet</option>
                                        <option value="2">Milimeter</option>
                                        <option value="3">Centimeter</option>
                                        <option value="4">Meter</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Weight: <span class="orange">(*)</span></label>
                            <div class="col-sm-9">
                                <input type="text" id="weight" name="weight" placeholder="weight" class="col-xs-12 col-sm-5"/>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1-1">Weight Unit: <span class="orange">(*)</span></label>
                            <div class="col-xs-12 col-sm-9">
                                <div class="col-xs-12 col-sm-5" style="margin-left: 0px; margin-right: 0px; padding-left: 0px; padding-right: 0px;">
                                    <select class="col-xs-12 col-sm-12" id="wunit" name="wunit" data-placeholder="Choose a unit...">
                                        <option value=""> Choose a unit... </option>
                                        <option value="0">oz</option>
                                        <option value="1">Pound</option>
                                        <option value="2">Gram</option>
                                        <option value="3">Kilogam</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        
                        <div class="form-group">
                            <label class="col-sm-3 control-label no-padding-right" for="form-field-1-1">Option Value: <span class="orange">(*)</span></label>
                            <div class="col-xs-12 col-sm-9">
                                <div class="col-xs-12 col-sm-5" style="margin-left: 0px; margin-right: 0px; padding-left: 0px; padding-right: 0px;">
                                    <select multiple="multiple" size="10" name="lov" id="lov">
                                        {{#loop_option_value}}
                                        <option value="{{KEY}}">{{VALUE}}</option>
                                        {{/loop_option_value}}
                                    </select>
                                </div>
                            </div>
                        </div>

                        <div class="form-group">
                            <label class="col-sm-3 col-xs-5 control-label no-padding-right" for="form-field-2">Is Taxable: <span class="orange">(*)</span></label>
                            <div class="col-sm-9 col-xs-7">
                                <label>
                                    <input id="istax" name="istax" checked class="ace ace-switch ace-switch-4" type="checkbox" />
                                    <span class="lbl"></span>
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 col-xs-5 control-label no-padding-right" for="form-field-2">Is Discount: <span class="orange">(*)</span></label>
                            <div class="col-sm-9 col-xs-7">
                                <label>
                                    <input id="isdis" name="isdis" class="ace ace-switch ace-switch-4" type="checkbox" />
                                    <span class="lbl"></span>
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 col-xs-5 control-label no-padding-right" for="form-field-2">Is Active: <span class="orange">(*)</span></label>
                            <div class="col-sm-9 col-xs-7">
                                <label>
                                    <input id="isact" name="isact" checked class="ace ace-switch ace-switch-4" type="checkbox" />
                                    <span class="lbl"></span>
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 col-xs-5 control-label no-padding-right" for="form-field-2">Is Delete: <span class="orange">(*)</span></label>
                            <div class="col-sm-9 col-xs-7">
                                <label>
                                    <input id="isdel" name="isdel" class="ace ace-switch ace-switch-4" type="checkbox" />
                                    <span class="lbl"></span>
                                </label>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 col-xs-3 control-label no-padding-right" for="form-field-2">Primary Photo: <span class="orange">(*)</span></label>
                            <div class="col-sm-9 col-xs-9">
                                <div class="col-xs-12 col-sm-5">
                                    <input type="file" name="priphoto" id="priphoto" style="display: none;" />
                                </div>
                            </div>
                        </div>
                        <div class="form-group">
                            <label class="col-sm-3 col-xs-3 control-label no-padding-right" for="form-field-2">Other Photo: <span class="orange">(*)</span></label>
                            <div class="col-sm-9 col-xs-9">
                                <div class="col-xs-10 col-sm-5">
                                    <input type="file" name="photo1" id="photo1" style="display: none;" />
                                </div>
                                <div class="col-xs-2 col-sm-5" style="padding-left: 0px;">
                                    <button id="btnap" class="btn btn-xs btn-success tooltip-success" type="button" onclick="addPhoto(); return false;" data-rel="tooltip" title="Add" data-placement="top"><i class="ace-icon fa fa-plus bigger-110"></i></button>
                                </div>
                            </div>
                        </div>
                        <div id="listPhoto">
                        </div>

                        <div class="clearfix">
                            <div class="col-md-offset-3 col-md-9">
                                <button id="btnadd" class="btn btn-info" type="button" onclick="addSKU(); return false;"><i class="ace-icon fa fa-check bigger-110"></i>Add</button>
                                &nbsp; &nbsp; &nbsp;
                                <button class="btn" type="reset" onclick="resetAdd(); return false;"><i class="ace-icon fa fa-undo bigger-110"></i>Reset</button>
                            </div>
                        </div>
                        <input type="hidden" id="numPhoto" name="numPhoto" value="1"/>
                        <input type="hidden" id="ov" name="ov" value=""/>
                        <input type="hidden" id="action" name="action" value="addsku"/>
                        <input type="hidden" id="callback" name="callback" value="completeAddSKU"/>
                    </form>
                </div>
            </div>

            <h3 class="header smaller lighter blue">List SKUs</h3>

            <div class="row">
                <div class="col-xs-12">
                    <table id="tableproduct" class="table table-striped table-bordered">
                        <thead>
                            <tr>
                                <th class="center">No.</th>
                                <th class="hidden-480">Photo</th>
                                <th class="hidden-md hidden-lg">Name</th>
                                <!--<th class="hidden-480">Product</th>-->
                                <th class="hidden-480">SKU Info</th>
                                <th class="hidden-480">SKU Info</th>
                                <th class="hidden-480">Description</th>
                                <!--<th class="hidden-480">Width x Height x Depth</th>-->
                                <!--<th class="hidden-480">Weight</th>-->
                                <th class="hidden-480">Option Value</th>
                                <!--<th>Flags</th>-->
                                <th class="hidden-480"><i class="ace-icon fa fa-clock-o bigger-110 hidden-480"></i>Update</th>
                                <th></th>
                            </tr>
                        </thead>

                        <tbody>
                            {{#loop_row}}
