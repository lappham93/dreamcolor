
<h3 class="header smaller lighter blue">Add Video</h3>
<div class="row">
    <div class="col-xs-12">
        <iframe id="if_Add" name="if_Add" style="display: none;width: 0px;height: 0px"></iframe>
        <form id="frmAdd" name="frmAdd" class="form-horizontal" role="form" action="/web/admin/video" method="POST" enctype="multipart/form-data" target="if_Add">
            <div id="add_alert_success" class="alert alert-success" style="display: none;"></div>
            <div id="add_alert_err" class="alert alert-danger" style="display: none;"></div>
            <div class="form-group">
                <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Link: <span class="orange">(*)</span></label>
                <div class="col-sm-9">
                    <input type="text" id="link" name="link" placeholder="link" class="col-xs-12 col-sm-5"/>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Title: <span class="orange">(*)</span></label>
                <div class="col-sm-9">
                    <input type="text" id="title" name="title" placeholder="Title" class="col-xs-12 col-sm-5"/>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3 control-label no-padding-right" for="form-field-1">Description: <span class="orange">(*)</span></label>
                <div class="col-sm-9">
                    <input type="text" id="desc" name="desc" placeholder="description" class="col-xs-12 col-sm-5"/>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3 col-xs-5 control-label no-padding-right" for="form-field-2">Status: <span class="orange">(*)</span></label>
                <div class="col-sm-9 col-xs-7">
                    <label>
                        <input id="status" name="status" checked class="ace ace-switch ace-switch-4" type="checkbox" />
                        <span class="lbl"></span>
                    </label>
                </div>
            </div>
            <div class="form-group">
                <label class="col-sm-3 col-xs-3 control-label no-padding-right" for="form-field-2">Thumb: <span class="orange">(*)</span></label>
                <div class="col-sm-9 col-xs-9">
                    <div class="col-xs-12 col-sm-5">
                        <input type="file" name="thumb" id="thumb" style="display: none;" />
                    </div>
                </div>
            </div>

            <div class="clearfix">
                <div class="col-md-offset-3 col-md-9">
                    <button id="btnadd" class="btn btn-info" type="button" onclick="addVideo(); return false;"><i class="ace-icon fa fa-check bigger-110"></i>Add</button>
                    &nbsp; &nbsp; &nbsp;
                    <button class="btn" type="reset" onclick="resetAdd(); return false;"><i class="ace-icon fa fa-undo bigger-110"></i>Reset</button>
                </div>
            </div>
            <input type="hidden" id="numPhoto" name="numPhoto" value="1"/>
            <input type="hidden" id="ov" name="ov" value=""/>
            <input type="hidden" id="action" name="action" value="addvideo"/>
            <input type="hidden" id="callback" name="callback" value="completeAddVideo"/>
        </form>
    </div>
</div>

