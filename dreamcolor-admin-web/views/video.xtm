    function resetAdd(){
        $('#add_alert_success').hide();
        $('#add_alert_err').hide();
        $('#url').val("");
        $('#desc').val("");
        $('#title').val("");
        $('#status').prop('checked', true);
        $('#ov').val("");
        $('#ov').val('').trigger('chosen:updated');
        $('.removeall').click();
        $('input[id*="thumb"]').ace_file_input('reset_input');
        $('#btnadd').button('reset');
        $("html, body").animate({ scrollTop: 0 }, "slow");
    }
    
    function addVideo(){
        $('#add_alert_success').hide();
        $('#add_alert_err').hide();
        var serr = "";
        var link = $('#link').val();
        if(link == ''){
            serr += "Link required. ";
        }
        var title = $('#title').val();
        if(title == ''){
            serr += "Title required. ";
        }
        var desc = $('#desc').val();
        if(desc == ''){
            serr += "Description required. ";
        }
        var photo = $('#thumb').val();
        if(photo == ''){
            serr += "Thumb required. ";
        }

//        $('#numPhoto').val(numPhoto);

        $('#frmAdd').unbind('submit');
        if(serr == ''){
            console.log('frmAdd submit...');
            $("#btnadd").button('loading');
            $('#frmAdd').submit();
        } else{
            $('#add_alert_err').html(serr);
            $('#add_alert_err').show();
            $("html, body").animate({ scrollTop: 0 }, "slow");
        }
        return false;
    };
    
    function completeAddVideo(data){
        if(data.err >= 0){
            $('#add_alert_success').html(data.msg);
            $('#add_alert_success').show();
            $("html, body").animate({ scrollTop: 0 }, "slow");

            setTimeout(function() {
                window.location.reload(true);
            }, 2000);
        } else{
            $('#add_alert_err').html(data.msg);
            $('#add_alert_err').show();
            $("html, body").animate({ scrollTop: 0 }, "slow");

            $('#frmAdd').bind('submit');
            $('#btnadd').button('reset');
        }
    };
    
    //Edit.
    function openEditModal(id){
        $('#eid').val(id);
        $('#edit_alert_success').hide();
        $('#edit_alert_err').hide();
        
        if(id != ''){
            var data = {
                action: 'getvideo',
                id: id
            };
            $.ajax({
                type: "POST",
                url: "{{domain}}/web/admin/video",
                data: data,
                dataType : 'json',
                cache: false,
                success: function(data){
                    if(data.err >= 0){
                        var sku = data.video;
                        $('#elink').val(sku.link);
                        $('#etitle').val(sku.title);
                        $('#edesc').val(sku.desc);
                        $('#estatus').prop('checked', sku.status == 'on');
                    } else{
                        console.log(data.msg);
                        bootbox.dialog({
                            message: data.msg,
                            buttons: {
                                "success" : {
                                    "label" : "OK",
                                    "className" : "btn-sm btn-primary"
                                }
                            }
                        });
                        return false;
                    }
                }
            });
        }
        
        $('#editModal').modal({backdrop: 'static', keyboard: false, show: true});
    };
    
